{
    "env": {
        "browser": true
    },
    "extends": "airbnb-base",
    "rules": {
      "no-restricted-syntax": [
        "error",
        "LabeledStatement",
        "WithStatement"
      ],
      "prefer-destructuring": [
        "error",
        {
          "array": false,
          "object": false
        }
      ],
      "prefer-arrow-callback": [
        0
      ],
      "func-names": [
        0
      ]
    }
}
